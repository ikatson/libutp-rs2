{
  "lsp": {
    "rust-analyzer": {
      // "binary": {
      //   "path": "rust-analyzer-aarch64-apple-darwin-2025-01-20"
      // },
      "initialization_options": {
        "check": {
          "command": "clippy" // rust-analyzer.check.command (default: "check")
        }
      },
      "cargo": {
        "loadOutDirsFromCheck": true
      }
    }
  }
}
